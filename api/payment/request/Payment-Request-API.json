          "pattern": "^(?:[A-Za-z0-9+/]{4})*(?:[A-Za-z0-9+/]{2}==|[A-Za-z0-9+/]{3}=)?$",
          "type": "string",
          "x-ing-type": "string"
        },
        "merchantName": {
          "description": "",
          "example": "Company BV",
