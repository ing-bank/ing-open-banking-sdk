          "type": "string",
          "x-ing-type": "string"
        },
        "merchantId": {
          "description": "Identification code of the merchant iDEAL contract.",
          "example": "001234567",
          "pattern": "^[0-9]{9}$",
          "type": "string",
          "x-ing-type": "string"
        },
        "merchantLogo": {
          "description": "Base64 encoded company logo.",
