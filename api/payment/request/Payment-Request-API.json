          "minLength": 1,
          "pattern": "^[-A-Za-z0-9= %*+,./&@\\\"\\':;?()$]*$",
          "type": "string",
          "x-ing-type": "string"
        },
        "merchantSubId": {
          "description": "Sub identification code of the merchant iDEAL contract.",
          "example": "123456",
          "maxLength": 6,
          "pattern": "^[0-9]*$",
          "type": "string",
          "x-ing-type": "string"
        },
        "status": {
          "description": "Status of this merchant registration. This is set to Active once the iDEAL contract certificate has been uploaded to the customer's iDEAL dashboard",
