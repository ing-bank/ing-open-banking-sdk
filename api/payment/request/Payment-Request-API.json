{
  "consumes": [
    "application/json"
  ],
  "definitions": {
    "CreatePaymentRequest": {
      "properties": {
        "description": {
          "description": "The description as shown on the payment request screen. Maximum length of 1024 characters.",
          "example": "Your purchase reference abcdefg1234567890",
          "maxLength": 1024,
          "type": "string",
          "x-ing-type": "string"
        },
        "fixedAmount": {
          "$ref": "#/definitions/fixedAmount"
        },
        "maximumAllowedPayments": {
          "description": "Maximum number of successful payments allowed for this payment request. If this number is reached, the status of the payment request will be *fulfilled*.",
          "example": 10,
          "minimum": 1,
          "type": "integer",
          "x-ing-type": "integer"
        },
        "maximumReceivableAmount": {
          "$ref": "#/definitions/maximumReceivableAmount"
        },
        "purchaseId": {
          "description": "Consumer generated identifier of the transaction as shown in the iDEAL issuer screens and payers' transaction overview.",
          "example": "abcdefg1234567890",
          "maxLength": 35,
          "pattern": "^[a-zA-Z0-9]*$",
          "type": "string",
          "x-ing-type": "string"
        },
        "returnUrl": {
          "description": "URL to which payers will be redirected after completing or cancelling the iDEAL payment. Payment request identifier (prid) and iDEAL transaction identifiers (trxid and ec) will be added as query parameters, as such: *https://www.webshop.com/return?purchaseId=abcdefg1234567890&prid=xrgq1coyp1pnnfigiaaa6pqn1o9lc0gh&trxid=0050000123456789&ec=abcdef01234567890*. Default if left blank is *https://www.ing.nl/particulier/betaalverzoek/return.html*.",
          "example": "https://www.webshop.com/return?purchaseId=abcdefg1234567890",
          "format": "uri",
          "pattern": "^(https)://[-a-zA-Z0-9+&@#\\/%?=~_|!:,.;]*[-a-zA-Z0-9+&@#/%=~_|]",
          "type": "string",
          "x-ing-type": "string"
        },
        "validUntil": {
          "description": "Expiration date-time of this payment request, with support for time zones. This must be a date-time in the future, with a maximum of one year.",
          "example": "2020-12-31T00:00:00.000+01:00",
          "format": "date-time",
          "type": "string",
          "x-ing-type": "string"
        },
        "variableAmount": {
          "$ref": "#/definitions/variableAmount"
        }
      },
      "required": [
        "description",
        "purchaseId",
        "validUntil"
      ],
      "type": "object"
    },
    "CreatePaymentRequestResponse": {
      "properties": {
        "id": {
          "description": "Unique identifier of the created payment request.",
          "example": "xrgq1coyp1pnnfigiaaa6pqn1o9lc0gh",
          "type": "string",
          "x-ing-type": "string"
        },
        "paymentInitiationUrl": {
          "description": "URL to which payers should be directed to initiate a payment.",
          "example": "https://www.ing.nl/zakelijk/betaalverzoek/index.html?trxid=xrgq1coyp1pnnfigiaaa6pqn1o9lc0gh",
          "format": "uri",
          "type": "string",
          "x-ing-type": "string"
        }
      },
      "type": "object"
    },
    "GetPaymentRequestResponse": {
      "properties": {
        "description": {
          "description": "The description as shown on the payment request screen. Maximum length of 1024 characters.",
          "example": "Your purchase reference abcdefg1234567890",
          "type": "string",
          "x-ing-type": "string"
        },
        "fixedAmount": {
          "$ref": "#/definitions/fixedAmount"
        },
        "id": {
          "description": "Unique identifier of this payment request.",
          "example": "xrgq1coyp1pnnfigiaaa6pqn1o9lc0gh",
          "type": "string",
          "x-ing-type": "string"
        },
        "maximumAllowedPayments": {
          "description": "Maximum number of successful payments allowed for this payment request. If this number is reached, the status of the payment request will be *fulfilled*.",
          "example": 1,
          "type": "integer",
          "x-ing-type": "integer"
        },
        "maximumReceivableAmount": {
          "$ref": "#/definitions/maximumReceivableAmount"
        },
        "paymentInitiationUrl": {
          "description": "URL to which payers should be directed to initiate payment.",
          "example": "https://www.ing.nl/zakelijk/betaalverzoek/index.html?trxid=xrgq1coyp1pnnfigiaaa6pqn1o9lc0gh",
          "format": "uri",
          "pattern": "^(https)://[-a-zA-Z0-9+&@#\\/%?=~_|!:,.;]*[-a-zA-Z0-9+&@#/%=~_|]",
          "type": "string",
          "x-ing-type": "string"
        },
        "purchaseId": {
          "description": "Consumer generated identifier of the transaction as shown in the iDEAL issuer screens and payers' transaction overview.",
          "example": "abcdefg1234567890",
          "type": "string",
          "x-ing-type": "string"
        },
        "returnUrl": {
          "description": "URL to which payers will be redirected after completing or cancelling the iDEAL payment. Payment request identifier (prid) and iDEAL transaction identifiers (trxid and ec) will be added as query parameters, as such: https://www.webshop.com/return?purchaseId=abcdefg1234567890&prid=xrgq1coyp1pnnfigiaaa6pqn1o9lc0gh&trxid=0050000123456789&ec=abcdef01234567890.",
          "example": "https://www.webshop.com/return?purchaseId=abcdefg1234567890",
          "format": "uri",
          "pattern": "^(https)://[-a-zA-Z0-9+&@#\\/%?=~_|!:,.;]*[-a-zA-Z0-9+&@#/%=~_|]",
          "type": "string",
          "x-ing-type": "string"
        },
        "status": {
          "description": "Status of this payment request. Possible values are open, expired, closed or fulfilled. It is not possible to perform payments for closed, expired, or fulfilled payment requests.",
          "enum": [
            "open",
            "closed",
            "expired",
            "fulfilled"
          ],
          "example": "open",
          "type": "string",
          "x-ing-type": "string"
        },
        "transactions": {
          "description": "DEPRECATED: List of payments for this payment request.",
          "items": {
            "$ref": "#/definitions/Payment"
          },
          "minItems": 0,
          "type": "array",
          "x-ing-type": "array"
        },
        "validUntil": {
          "description": "Expiration date-time of this payment request.",
          "example": "2020-12-31T00:00:00.000+01:00",
          "format": "date-time",
          "type": "string",
          "x-ing-type": "string"
        },
        "variableAmount": {
          "$ref": "#/definitions/variableAmount"
        }
      },
      "type": "object"
    },
    "GetPaymentsResponse": {
      "description": "List of payments for this payment request.",
      "properties": {
        "payments": {
          "items": {
            "$ref": "#/definitions/Payment"
          },
          "minItems": 0,
          "type": "array",
          "x-ing-type": "array"
        }
      },
      "type": "object"
    },
    "GetRegistrationResponse": {
      "properties": {
        "allowIngAppPayments": {
          "description": "Possibility to pay a payment request with the ING mobile app (Y/N).",
          "example": "Y",
          "type": "string",
          "x-ing-type": "string"
