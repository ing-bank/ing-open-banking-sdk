    "dailyReceivableLimit": {
      "description": "Maximum amount that can be received by this merchant per day. Value should be formatted with either zero or two decimal places and must be between 0.01 and 50.000 euros. ",
      "properties": {
        "currency": {
          "description": "Currency of the daily receivable limit. Currently the only supported value is *EUR* (Euros).",
          "example": "EUR",
          "pattern": "EUR",
          "type": "string",
          "x-ing-type": "string"
        },
        "value": {
          "description": "Value of the daily receivable limit.",
          "example": 25000.0,
          "maximum": 50000.0,
          "minimum": 0.01,
          "type": "number",
          "x-ing-type": "number"
        }
      },
      "required": [
        "currency",
        "value"
      ],
      "type": "object"
    },
    "error_1.0": {
      "properties": {
        "error": {
          "description": "Error message",
          "properties": {
            "code": {
              "description": "Service-specific error code/keyword",
              "example": "errParseFailed",
              "type": "string",
              "x-ing-type": "string"
            },
            "innerErrors": {
              "items": {
                "description": "Upstream error messages and detailed information on error state",
                "properties": {
                  "code": {
                    "description": "Service-specific error code/keyword",
                    "example": "errParseFailed",
                    "type": "string",
                    "x-ing-type": "string"
                  },
                  "message": {
                    "description": "A descriptive error message for the consumer",
                    "example": "Unexpected end of file (missing a terminator/close bracket?)",
                    "type": "string",
                    "x-ing-type": "string"
                  },
                  "severity": {
                    "enum": [
                      "critical",
                      "error",
                      "warning",
                      "info"
                    ],
                    "type": "string",
                    "x-ing-type": "string"
                  },
                  "source": {
                    "description": "Optional source descriptor for this error",
                    "example": "system76",
                    "type": "string",
                    "x-ing-type": "string"
                  },
                  "target": {
                    "description": "Key/value pair indicating which input property caused the error",
                    "type": "string",
                    "x-ing-type": "string"
                  }
                },
                "required": [
                  "code",
                  "message",
                  "severity"
                ],
                "type": "object",
                "x-ing-type": "object"
              },
              "type": "array",
              "x-ing-type": "array"
            },
